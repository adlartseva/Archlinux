###############################
## Группа пакетов Base-devel ##
###############################

autoconf # инструмент GNU для авто конфигурирования исходного кода

automake  # инструмент GNU для авто создания Makefiles

binutils  # набор программ для сборки и манипулирования бинарных и объектных файлов

bison  # генератор синтактических анализаторов общего назначения GNU

fakeroot  # инструмент для моделирования привилегий суперпользователя

file  # утилита идентификации типа файла

findutils  # GNU утилита для поиска файлов

flex  # инструмент для генерации программ сканирования текста

gawk  # GNU версия AWK

gcc  # GNU компилятор С и С++

gettext  # библиотека интернационализация GNU

grep  # утилита строки поиска

groff  # система формирования текста TROFF GNU

gzip  # утилита сжатия GNU

libtool  # сценарий поддержки родных библиотек

m4  # процессор GNU макро

make  # GNU make утилита для поддержания группы пакетов

pacman  # библиотека на основе менеджера пакетов с поддержкой зависимостей

path  # утилита для применения файлов исправлний к оригинальным источникам

pkg-config  # система управления библиотекой флагами компиляции / сборки

sed  # GNU потоковый редактор

sudo  # дает определенным пользователям возможность запускать некоторые команды, как суперпользователь

texinfo  # система документации GNU для оперативной информации и пчатной продукции

util-linux  # различные системные утилиты дл Linux

which  # утилита, чтобы показать полный путь команд